{
    "name": "webleit/zohobooksapi",
    "description": "Zoho Books API v3 - PHP SDK",
    "license": "MIT",
    "keywords": ["zoho", "books", "zohobooks", "api"],
    "authors": [
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it"
        }
    ],
    "require": {
        "php" : "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "^6.0 | ^7.0",
        "doctrine/inflector": "^2.0",
        "illuminate/collections": "^v8.0",
        "weble/zohoclient": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "cache/filesystem-adapter": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Webleit\\ZohoBooksApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webleit\\ZohoBooksApi\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
